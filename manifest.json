{
  "manifest_version": 3,
  "name": "Multi-Chat Deletion for ChatGPT",
  "version": "1.0",
  "description": "Enables multi-chat selection and deletion for ChatGPT web interface.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.openai.com/chat*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
